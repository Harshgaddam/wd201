    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4"
  },
  "dependencies": {
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
